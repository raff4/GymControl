<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:with type="com.bertazoli.client.place.NameTokens" field="nametokens"></ui:with>

	<g:HTMLPanel>
		<g:InlineLabel>Username:</g:InlineLabel><br />
		<g:TextBox ui:field="username"/><br />
		<g:InlineLabel>Password:</g:InlineLabel><br />
		<g:PasswordTextBox ui:field="password"/><br />
		<g:Button ui:field="send">Send</g:Button>
		<br />
		OR
		<br />
		<g:InlineHyperlink targetHistoryToken="{nametokens.getSignup}">Sign Up</g:InlineHyperlink>
	</g:HTMLPanel>
</ui:UiBinder>
