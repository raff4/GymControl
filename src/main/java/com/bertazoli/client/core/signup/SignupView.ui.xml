<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
	<h1>Create a new account</h1>
	<g:InlineLabel>Username:</g:InlineLabel><br />
	<g:TextBox ui:field="username" /><br />
	<g:InlineLabel>Password:</g:InlineLabel><br />
	<g:PasswordTextBox ui:field="password" /><br />
	<g:InlineLabel>Confirm password:</g:InlineLabel><br />
	<g:PasswordTextBox ui:field="confirmPassword" /><br />
	<g:InlineLabel>First name:</g:InlineLabel><br />
	<g:TextBox ui:field="firstname" /><br />
	<g:InlineLabel>Last name:</g:InlineLabel><br />
	<g:TextBox ui:field="lastname" /><br />
	<g:InlineLabel>email:</g:InlineLabel><br />
	<g:TextBox ui:field="email" /><br />
	<g:InlineLabel>Date of birth:</g:InlineLabel><br />
	<d:DateBox ui:field="dob" /><br />
	<g:Button ui:field="send">Send</g:Button>
	</g:HTMLPanel>
</ui:UiBinder>
