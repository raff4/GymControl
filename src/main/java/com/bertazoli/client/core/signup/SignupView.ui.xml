<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
	<h1>Create a new account</h1>
	<g:TextBox ui:field="username" />
	<g:PasswordTextBox ui:field="password" />
	<g:PasswordTextBox ui:field="confirmPassword" />
	<g:TextBox ui:field="firstname" />
	<g:TextBox ui:field="lastname" />
	<g:TextBox ui:field="email" />
	<d:DateBox ui:field="dob" />
	<g:Button ui:field="send">Send</g:Button>
	</g:HTMLPanel>
</ui:UiBinder>
