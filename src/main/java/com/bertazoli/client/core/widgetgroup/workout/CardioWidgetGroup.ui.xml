<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="gradientBackground2 box padding5">
		<g:InlineLabel>Name:</g:InlineLabel>
		<g:TextBox ui:field="name"/>
		<g:InlineLabel>Hours:</g:InlineLabel>
		<g:TextBox ui:field="hours" styleName="width25" maxLength="2"/>
		<g:InlineLabel>Minutes:</g:InlineLabel>
		<g:TextBox ui:field="minutes" styleName="width25" maxLength="2"/>
		<g:Image ui:field="delete" styleName="marginLeft10 pointer" url="images/delete.png"/>
	</g:HTMLPanel>
</ui:UiBinder>