<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="gradientBackground2 box padding5 relative">
		<g:InlineLabel>Name:</g:InlineLabel>
		<g:TextBox ui:field="name"/>
		<g:InlineLabel>Weight:</g:InlineLabel>
		<g:TextBox ui:field="weight" styleName="width50" maxLength="4"/>
		<g:InlineLabel>Sets:</g:InlineLabel>
		<g:TextBox ui:field="sets" styleName="width25" maxLength="2"/>
		<g:InlineLabel>Repetitions:</g:InlineLabel>
		<g:TextBox ui:field="repetitions" styleName="width25" maxLength="2"/>
		<g:Image ui:field="delete" styleName="marginLeft10 pointer delete" url="images/delete.png"/>
	</g:HTMLPanel>
</ui:UiBinder>